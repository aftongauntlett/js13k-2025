{
  "name": "js13k-2025-firefly-cat",
  "version": "1.0.0",
  "description": "A magical firefly collection game for JS13K 2025",
  "main": "dist/index.html",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "build": "npm run minify && npm run compress && npm run zip",
    "minify": "terser main.js -c -m -o dist/main.min.js",
    "roadroller": "roadroller dist/main.min.js -o dist/main.roadroller.js",
    "inline": "node build/inline.js",
    "compress": "npm run roadroller && npm run inline",
    "zip": "cd dist && zip ../js13k-2025.zip index.html && cd .. && ls -la js13k-2025.zip && echo 'Zip size:' && wc -c js13k-2025.zip",
    "size": "wc -c js13k-2025.zip && echo 'Limit: 13312 bytes'",
    "test": "npm run build && npm run size"
  },
  "devDependencies": {
    "roadroller": "^2.1.0",
    "terser": "^5.43.1"
  },
  "keywords": [
    "js13k",
    "game",
    "javascript",
    "canvas"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.15.0",
    "buffer-from": "^1.1.2",
    "commander": "^2.20.3",
    "esm": "^3.2.25",
    "source-map": "^0.6.1",
    "source-map-support": "^0.5.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aftongauntlett/js13k-2025.git"
  },
  "bugs": {
    "url": "https://github.com/aftongauntlett/js13k-2025/issues"
  },
  "homepage": "https://github.com/aftongauntlett/js13k-2025#readme"
}
